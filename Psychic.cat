    <selectionEntry type="upgrade" import="true" name="Divination" hidden="false" id="5f50-43e2-30a1-01c8">
      <categoryLinks>
        <categoryLink targetId="c457-4d05-c6b9-9a82" id="8a91-cafe-9f3e-1fcc" primary="false" name="Diviner"/>
